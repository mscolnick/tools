{
  "name": "@mscolnick/ts-morph-tools",
  "version": "0.0.0",
  "description": "A toolset of commands for ts-morph",
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "sideEffects": false,
  "bin": {
    "ts-morph-tools": "./bin/cli"
  },
  "scripts": {
    "build": "tsc -p src/",
    "dev": "tsc -w -p src/",
    "prettier": "prettier --list-different 'src/**/*.{ts,tsx,json,md}'",
    "test": "jest",
    "lint": "tsc -p src/ --noEmit --skipLibCheck; tslint --fix 'src/**/*.{ts,tsx}' -p src/"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "lodash-es": "^4.17.21",
    "ts-morph": "^5.0.0",
    "tslib": "^1.9.3",
    "yargs": "^15.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/lodash-es": "^4.14.138",
    "@types/node": "^12.0.0",
    "@types/tempy": "^0.3.0",
    "@types/yargs": "^13.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "tempy": "^0.3.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  }
}
